

public interface Carte {
	
	/** Obtenir le numéro de la carte.
	 * @return  le numéro de la carte
	 */
	int getNumero();
	
	/** Obtenir le nom du fichier image de la carte
	 * @return le nom du fichier image
	 */
	public String getNomImg();
	
	/** Informer si la carte a été découverte.
	 * @return un boolean informant si la carte a été découverte.
	 */
	boolean estDecouverte(); //add
	
	/** Découvrir la carte.
	 * @param le numéro de la carte.
	 */
	
	void decouvrir(); //add

	/** Afficher la carte
	 * @return la carte
	 */
	void defausser();
}
